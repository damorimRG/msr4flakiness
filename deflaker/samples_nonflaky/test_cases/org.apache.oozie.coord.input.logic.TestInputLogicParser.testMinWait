@Test
public void testMinWait() throws JDOMException {
    // @formatter:off
    String xml = "<input-logic>" + "<and name=\"test\" min=\"3\" wait=\"10\">" + "<data-in dataset=\"A\"/> " + "<data-in dataset=\"B\"/>" + "</and>" + "</input-logic>";
    // @formatter:on
    Element root = XmlUtils.parseXml(xml);
    InputLogicParser inputLogicParser = new InputLogicParser();
    assertEquals("(dependencyBuilder.input(\"A\").min(3).inputWait(10).build() " + "&& dependencyBuilder.input(\"B\").min(3).inputWait(10).build())", inputLogicParser.parseWithName(root, "test"));
    assertEquals("(dependencyBuilder.input(\"A\").min(3).inputWait(10).build() " + "&& dependencyBuilder.input(\"B\").min(3).inputWait(10).build())", inputLogicParser.parse(root));
}
