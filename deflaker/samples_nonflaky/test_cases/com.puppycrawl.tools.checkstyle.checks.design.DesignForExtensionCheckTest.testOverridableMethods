@Test
public void testOverridableMethods() throws Exception {
    final DefaultConfiguration checkConfig = createModuleConfig(DesignForExtensionCheck.class);
    final String[] expected = { "6:9: " + getCheckMessage(MSG_KEY, "A", "foo1"), "30:9: " + getCheckMessage(MSG_KEY, "A", "foo8"), "35:9: " + getCheckMessage(MSG_KEY, "A", "foo9"), "42:9: " + getCheckMessage(MSG_KEY, "A", "foo10"), "49:9: " + getCheckMessage(MSG_KEY, "A", "foo11"), "54:9: " + getCheckMessage(MSG_KEY, "A", "foo12"), "61:9: " + getCheckMessage(MSG_KEY, "A", "foo13"), "68:9: " + getCheckMessage(MSG_KEY, "A", "foo14"), "90:9: " + getCheckMessage(MSG_KEY, "A", "foo22"), "96:9: " + getCheckMessage(MSG_KEY, "A", "foo23"), "105:9: " + getCheckMessage(MSG_KEY, "A", "foo25"), "110:9: " + getCheckMessage(MSG_KEY, "A", "foo26"), "117:9: " + getCheckMessage(MSG_KEY, "A", "foo27"), "129:9: " + getCheckMessage(MSG_KEY, "A", "foo29"), "151:9: " + getCheckMessage(MSG_KEY, "A", "foo31"), "162:9: " + getCheckMessage(MSG_KEY, "A", "foo33"), "168:9: " + getCheckMessage(MSG_KEY, "A", "foo34"), "190:9: " + getCheckMessage(MSG_KEY, "A", "foo39"), "197:9: " + getCheckMessage(MSG_KEY, "A", "foo41") };
    verify(checkConfig, getPath("InputDesignForExtensionOverridableMethods.java"), expected);
}
