@Test
public void testGetDSTOffsetOfNonDaylightChangeGivesZeroOffset() throws ParseException {
    final long dstOffset = DaylightOffsetCalculator.getDSTOffset(TZ_DST, DateUtils.parseDateOozieTZ("2013-07-18T00:00Z"), DateUtils.parseDateOozieTZ("2013-07-19T00:00Z"));
    assertEquals("non-daylight change should give zero DST offset", 0L, dstOffset);
}
