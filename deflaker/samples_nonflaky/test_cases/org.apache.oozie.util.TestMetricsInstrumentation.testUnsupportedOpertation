public void testUnsupportedOpertation() {
    MetricsInstrumentation instr = new MetricsInstrumentation();
    try {
        instr.getAll();
        fail();
    } catch (UnsupportedOperationException uoe) {
    }
    try {
        instr.getSamplers();
        fail();
    } catch (UnsupportedOperationException uoe) {
    }
    try {
        instr.getTimers();
        fail();
    } catch (UnsupportedOperationException uoe) {
    }
    try {
        instr.getVariables();
        fail();
    } catch (UnsupportedOperationException uoe) {
    }
}
