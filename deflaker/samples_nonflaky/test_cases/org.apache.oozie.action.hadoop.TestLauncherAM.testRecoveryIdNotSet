@Test
public void testRecoveryIdNotSet() throws Exception {
    launcherJobConfig.unset(LauncherAMUtils.OOZIE_ACTION_RECOVERY_ID);
    instantiateLauncher();
    executeLauncher();
    failureDetails.expectedExceptionMessage("IO error").expectedErrorCode(EXIT_CODE_0).expectedErrorReason("IO error").withStackTrace();
    assertFailedExecution();
}
