@Test
public void testCheckVersionInvalid() {
    for (String v : new String[] { "1.0.0", "2.0.0", "2.5.0" }) {
        status.setVersion(v);
        try {
            status.checkVersion();
            fail("Version " + v + " shouldn't be allowed");
        } catch (Exception e) {
        }
    }
}
