public void testWhenChangingDSTCronAndELEveryThirdMonthFrequenciesEqual() throws Exception {
    Date startTime = DateUtils.parseDateOozieTZ("2016-01-01T13:10Z");
    Date endTime = DateUtils.parseDateOozieTZ("2017-12-03T00:00Z");
    String everyThirdMonth = "10 13 1 */3 *";
    Date[] nominalTimesWithTwoDstChange = new Date[] { DateUtils.parseDateOozieTZ(convertLATimeToUTC("2016-01-01T05:10")), // DST started on 13th March
    DateUtils.parseDateOozieTZ(convertLATimeToUTC("2016-04-01T05:10")), DateUtils.parseDateOozieTZ(convertLATimeToUTC("2016-07-01T05:10")), DateUtils.parseDateOozieTZ(convertLATimeToUTC("2016-10-01T05:10")), // DST ended on 6th of November
    DateUtils.parseDateOozieTZ(convertLATimeToUTC("2017-01-01T05:10")), // DST started again on 12th March
    DateUtils.parseDateOozieTZ(convertLATimeToUTC("2017-04-01T05:10")), DateUtils.parseDateOozieTZ(convertLATimeToUTC("2017-07-01T05:10")), DateUtils.parseDateOozieTZ(convertLATimeToUTC("2017-10-01T05:10")) };
    testELAndCronNominalTimesEqual(startTime, endTime, nominalTimesWithTwoDstChange, everyThirdMonth, "3", Timeunit.MONTH);
}
