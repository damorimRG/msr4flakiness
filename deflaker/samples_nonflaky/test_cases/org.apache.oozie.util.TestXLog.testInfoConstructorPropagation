public void testInfoConstructorPropagation() {
    XLog.Info.defineParameter("A");
    XLog.Info logInfo = new XLog.Info();
    logInfo.setParameter("A", "a");
    assertEquals("A[a]", logInfo.createPrefix());
    logInfo = new XLog.Info(logInfo);
    assertEquals("A[a]", logInfo.createPrefix());
}
