@Test
public void testGetAggregatedPrivileges() {
    assertAggregation(privilege.getAggregatePrivileges());
    assertAggregation(aggrPrivilege.getAggregatePrivileges(), JCR_NODE_TYPE_MANAGEMENT, JCR_WRITE, JCR_ADD_CHILD_NODES, JCR_REMOVE_CHILD_NODES, JCR_REMOVE_NODE, JCR_MODIFY_PROPERTIES, REP_ADD_PROPERTIES, REP_ALTER_PROPERTIES, REP_REMOVE_PROPERTIES);
}
