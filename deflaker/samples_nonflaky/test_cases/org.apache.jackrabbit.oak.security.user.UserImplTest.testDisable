@Test
public void testDisable() throws Exception {
    assertNull(user.getDisabledReason());
    assertFalse(user.isDisabled());
    user.disable("reason");
    assertEquals("reason", user.getDisabledReason());
    assertTrue(user.isDisabled());
    user.disable(null);
    assertNull(user.getDisabledReason());
    assertFalse(user.isDisabled());
}
