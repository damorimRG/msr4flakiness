@Test
public void testTokens() {
    final CatchParameterNameCheck catchParameterNameCheck = new CatchParameterNameCheck();
    final int[] expected = { TokenTypes.PARAMETER_DEF };
    assertArrayEquals(expected, catchParameterNameCheck.getRequiredTokens(), "Default required tokens are invalid");
    assertArrayEquals(expected, catchParameterNameCheck.getAcceptableTokens(), "Default acceptable tokens are invalid");
}
