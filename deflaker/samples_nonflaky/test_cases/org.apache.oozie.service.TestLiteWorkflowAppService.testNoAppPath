public void testNoAppPath() throws Exception {
    Services services = new Services();
    services.init();
    WorkflowAppService wps = services.get(WorkflowAppService.class);
    try {
        assertNotNull(wps.parseDef(new XConfiguration()));
        fail();
    } catch (Exception ex) {
    // nop
    } finally {
        services.destroy();
    }
}
