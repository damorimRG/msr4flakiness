@Test
public void testDmlAndOozieTableAppliesIgnoreCase() {
    assertTrue("a DML statement and of an Oozie table", statementPredicate.test("SELECT * FROM wf_jobs"));
    assertTrue("a DML statement and of an Oozie table", statementPredicate.test("insert into WF_JOBS"));
    assertTrue("a DML statement and of an Oozie table", statementPredicate.test("update wf_jobs"));
    assertTrue("a DML statement and of an Oozie table", statementPredicate.test("DELETE FROM WF_JOBS"));
}
