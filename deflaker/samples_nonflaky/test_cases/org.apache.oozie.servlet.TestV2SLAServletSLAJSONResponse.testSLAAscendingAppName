public void testSLAAscendingAppName() throws Exception {
    ListMultimap<String, String> filterParams = LinkedListMultimap.create();
    filterParams.put("job_status", "RUNNING");
    JSONArray array = getSLAJSONResponse(null, filterParams, "appName", false);
    // Matches all elements
    // 3-1-W 3-2-W 3-3-W 3-4-W 3-5-W 3-6-W
    // 2-1-W 2-2-W 2-3-W
    // 1-1-W and 1-2-W
    assertSLAJSONResponse(array, 0, 2, 1, 2, "1-", "-W", "1-C", nominalTime1, "test_app-1", AppType.WORKFLOW_JOB, currentTime);
    assertSLAJSONResponse(array, 2, 5, 1, 3, "2-", "-W", null, nominalTime2, "test_app-2", AppType.WORKFLOW_JOB, currentTime);
    assertSLAJSONResponse(array, 5, 11, 1, 6, "3-", "-W", "2-C", nominalTime3, "test_app-3", AppType.WORKFLOW_JOB, currentTime);
}
