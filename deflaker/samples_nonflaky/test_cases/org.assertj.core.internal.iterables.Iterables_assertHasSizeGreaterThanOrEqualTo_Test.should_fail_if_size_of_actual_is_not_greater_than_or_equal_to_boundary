@Test
public void should_fail_if_size_of_actual_is_not_greater_than_or_equal_to_boundary() {
    assertThatExceptionOfType(AssertionError.class).isThrownBy(() -> iterables.assertHasSizeGreaterThanOrEqualTo(someInfo(), actual, 6)).withMessage(shouldHaveSizeGreaterThanOrEqualTo(actual, actual.size(), 6).create());
}
