@Test
public void whenKilobytesAreGivenConvertedCorrectly() {
    assertEquals("kilobytes count should be converted correctly", 1L, new BytesAndUOMConverter().toMegabytes("1024K"));
    assertEquals("kilobytes count should be converted correctly", 0L, new BytesAndUOMConverter().toMegabytes("1023K"));
    assertEquals("kilobytes count should be converted correctly", 10L, new BytesAndUOMConverter().toMegabytes("10240K"));
}
