public void testResolve() {
    XConfiguration conf = new XConfiguration();
    conf.setRestrictSystemProperties(false);
    conf.set("a", "A");
    conf.set("b", "${a}");
    assertEquals("A", conf.getRaw("a"));
    assertEquals("${a}", conf.getRaw("b"));
    conf = conf.resolve();
    assertEquals("A", conf.getRaw("a"));
    assertEquals("A", conf.getRaw("b"));
}
