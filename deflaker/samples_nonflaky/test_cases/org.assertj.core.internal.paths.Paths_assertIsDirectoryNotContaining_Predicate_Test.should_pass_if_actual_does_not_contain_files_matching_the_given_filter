@Test
public void should_pass_if_actual_does_not_contain_files_matching_the_given_filter() {
    // GIVEN
    Path file = mockRegularFile("root", "Test.class");
    List<Path> items = list(file);
    Path actual = mockDirectory("root", items);
    // THEN
    paths.assertIsDirectoryNotContaining(INFO, actual, JAVA_SOURCE);
}
