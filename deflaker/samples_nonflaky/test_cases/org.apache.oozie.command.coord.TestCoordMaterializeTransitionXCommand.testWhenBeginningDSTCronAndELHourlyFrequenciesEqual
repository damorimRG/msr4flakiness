public void testWhenBeginningDSTCronAndELHourlyFrequenciesEqual() throws Exception {
    Date startTime = DateUtils.parseDateOozieTZ("2017-03-12T07:10Z");
    Date endTime = DateUtils.parseDateOozieTZ("2017-03-12T12:30Z");
    String everyHourAtTen = "10 * * * *";
    Date[] nominalTimesWithOneDstChange = new Date[] { DateUtils.parseDateOozieTZ(convertLATimeToUTC("2017-03-11T23:10")), DateUtils.parseDateOozieTZ(convertLATimeToUTC("2017-03-12T00:10")), DateUtils.parseDateOozieTZ(convertLATimeToUTC("2017-03-12T01:10")), // DST started
    DateUtils.parseDateOozieTZ(convertLATimeToUTC("2017-03-12T03:10")), DateUtils.parseDateOozieTZ(convertLATimeToUTC("2017-03-12T04:10")), DateUtils.parseDateOozieTZ(convertLATimeToUTC("2017-03-12T05:10")) };
    testELAndCronNominalTimesEqual(startTime, endTime, nominalTimesWithOneDstChange, everyHourAtTen, "1", Timeunit.HOUR);
}
