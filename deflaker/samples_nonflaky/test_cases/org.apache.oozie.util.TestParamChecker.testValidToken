@Test
public void testValidToken() {
    ParamChecker.validateActionName("azAZ09_-");
    try {
        ParamChecker.validateActionName(null);
        fail();
    } catch (IllegalArgumentException ex) {
    // nop
    }
    try {
        ParamChecker.validateActionName("");
        fail();
    } catch (IllegalArgumentException ex) {
    // nop
    }
    try {
        ParamChecker.validateActionName("@");
        fail();
    } catch (IllegalArgumentException ex) {
    // nop
    }
}
