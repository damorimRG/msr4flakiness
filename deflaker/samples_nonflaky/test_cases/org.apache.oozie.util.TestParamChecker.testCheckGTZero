@Test
public void testCheckGTZero() {
    assertEquals(120, ParamChecker.checkGTZero(120, "test"));
    try {
        ParamChecker.checkGTZero(0, "test");
        fail();
    } catch (Exception ex) {
    }
    try {
        ParamChecker.checkGTZero(-1, "test");
        fail();
    } catch (Exception ex) {
    }
}
