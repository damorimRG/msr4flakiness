@Test
public void testCheckInteger() {
    assertEquals(120, ParamChecker.checkInteger("120", "test"));
    assertEquals(-12, ParamChecker.checkInteger("-12", "test"));
    try {
        ParamChecker.checkInteger("ABCD", "test");
        fail();
    } catch (Exception ex) {
    }
    try {
        ParamChecker.checkInteger("1.5", "test");
        fail();
    } catch (Exception ex) {
    }
}
