@Test
public void testGetRequiredTokens() {
    final MissingJavadocTypeCheck missingJavadocTypeCheck = new MissingJavadocTypeCheck();
    assertArrayEquals(CommonUtil.EMPTY_INT_ARRAY, missingJavadocTypeCheck.getRequiredTokens(), "MissingJavadocTypeCheck#getRequiredTokens should return empty array by default");
}
