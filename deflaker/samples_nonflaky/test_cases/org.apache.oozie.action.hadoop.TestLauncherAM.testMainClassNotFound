@Test
public void testMainClassNotFound() throws Exception {
    launcherJobConfig.set(LauncherAM.CONF_OOZIE_ACTION_MAIN_CLASS, "org.apache.non.existing.Klass");
    executeLauncher();
    failureDetails.expectedExceptionMessage(ClassNotFoundException.class.getCanonicalName()).expectedErrorCode(EXIT_CODE_0).expectedErrorReason(ClassNotFoundException.class.getCanonicalName()).withStackTrace();
    assertFailedExecution();
}
