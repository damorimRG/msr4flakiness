@Test
public void testIsCheckstyleModule() {
    assertTrue(ModuleReflectionUtil.isCheckstyleModule(CheckClass.class), "Should return true when checkstyle module is passed");
    assertTrue(ModuleReflectionUtil.isCheckstyleModule(FileSetModuleClass.class), "Should return true when checkstyle module is passed");
    assertTrue(ModuleReflectionUtil.isCheckstyleModule(FilterClass.class), "Should return true when checkstyle module is passed");
    assertTrue(ModuleReflectionUtil.isCheckstyleModule(TreeWalkerFilterClass.class), "Should return true when checkstyle module is passed");
    assertTrue(ModuleReflectionUtil.isCheckstyleModule(FileFilterModuleClass.class), "Should return true when checkstyle module is passed");
    assertTrue(ModuleReflectionUtil.isCheckstyleModule(AuditListenerClass.class), "Should return true when checkstyle module is passed");
    assertTrue(ModuleReflectionUtil.isCheckstyleModule(RootModuleClass.class), "Should return true when checkstyle module is passed");
}
