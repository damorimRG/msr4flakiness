public void testWhenEndingDSTCronAndELHourlyFrequenciesEqual() throws Exception {
    Date startTime = DateUtils.parseDateOozieTZ("2017-11-05T07:10Z");
    Date endTime = DateUtils.parseDateOozieTZ("2017-11-05T10:30Z");
    String everyHourAtTen = "10 * * * *";
    Date[] nominalTimesWithOneDstChange = new Date[] { // LA time: 2017-11-05T00:10
    DateUtils.parseDateOozieTZ("2017-11-05T07:10Z"), // LA time: 2017-11-05T01:10
    DateUtils.parseDateOozieTZ("2017-11-05T08:10Z"), // LA time: 2017-11-05T01:10, DST ended
    DateUtils.parseDateOozieTZ("2017-11-05T09:10Z"), // LA time: 2017-11-05T02:10
    DateUtils.parseDateOozieTZ("2017-11-05T10:10Z") };
    testELAndCronNominalTimesEqual(startTime, endTime, nominalTimesWithOneDstChange, everyHourAtTen, "1", Timeunit.HOUR);
}
