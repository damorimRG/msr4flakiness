@Test
public void testActionThrowsLauncherException() throws Exception {
    setupArgsForMainClass(LAUNCHER_EXCEPTION);
    executeLauncher();
    failureDetails.expectedExceptionMessage(null).expectedErrorCode(String.valueOf(LAUNCHER_ERROR_CODE)).expectedErrorReason("exit code [" + LAUNCHER_ERROR_CODE + "]").withoutStackTrace();
    assertFailedExecution();
}
