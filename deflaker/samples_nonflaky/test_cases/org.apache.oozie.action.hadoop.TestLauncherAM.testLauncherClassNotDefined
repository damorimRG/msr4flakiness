@Test
public void testLauncherClassNotDefined() throws Exception {
    launcherJobConfig.unset(LauncherAM.CONF_OOZIE_ACTION_MAIN_CLASS);
    executeLauncher();
    failureDetails.expectedExceptionMessage("Launcher class should not be null").expectedErrorCode(EXIT_CODE_0).expectedErrorReason("Launcher class should not be null").withStackTrace();
    assertFailedExecution();
}
