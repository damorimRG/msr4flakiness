@Test
public void whenPrepareActionWithoutPrefixExecutedSuccessfully() throws SAXException, ParserConfigurationException, LauncherException, IOException {
    final String prepareXmlWithoutPrefix = "<prepare>\n" + "    <delete path=\"${nameNode}/user/${wf:user()}/${examplesRoot}/output-data/git/oozie\"/>\n" + "</prepare>";
    callAndVerifyPrepareAction(prepareXmlWithoutPrefix);
}
