@Test
public void whenPrepareActionWithGitPrefixExecutedSuccessfully() throws SAXException, ParserConfigurationException, LauncherException, IOException {
    final String prepareXmlWithGitPrefix = "<git:prepare xmlns:git=\"uri:oozie:git-action:1.0\">\n" + "    <git:delete path=\"${nameNode}/user/${wf:user()}/${examplesRoot}/output-data/git/oozie\"/>\n" + "</git:prepare>";
    callAndVerifyPrepareAction(prepareXmlWithGitPrefix);
}
