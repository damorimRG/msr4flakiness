@Test
public void testPrintShellInvalidShellCommand() throws UnsupportedEncodingException {
    callPrint(1, "invalid command");
    Assert.assertTrue(String.format("output stream must be empty but is [%s]", outputStream.toString(StandardCharsets.UTF_8.name())), outputStream.toString(StandardCharsets.UTF_8.name()).isEmpty());
    Assert.assertTrue(String.format("invalid error stream message [%s]", errorStream.toString(StandardCharsets.UTF_8.name())), errorStream.toString(StandardCharsets.UTF_8.name()).contains("doesn't appear to exist"));
}
