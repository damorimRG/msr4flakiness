@Test
public void testEncode() throws WriterException {
    doTest("ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789", "000001010111101101010001101001001101000101100101001100100101100010101011010001011001" + "001011000101001101001000110101010110001010011001010001101001011001000101101101101001" + "101100101101011001101001101100101101100110101011011001011001101001101101001110101000" + "101001010010001010001001010000101001010001001001001001000101010100001000100101000010" + "10100111010101000010101011110100000");
    doTest("\u0000\u0001\u001a\u001b\u001f $%+!,09:;@AZ[_`az{\u007f", "00000" + "101011110" + "111011010" + "110010110" + "100100110" + // bU aA
    "110101000" + "100100110" + "100111010" + "111011010" + // aZ bA
    "110101000" + "111011010" + "110010010" + "111010010" + // bE space $
    "111001010" + "110101110" + "101110110" + "111010110" + // % + cA
    "110101000" + "111010110" + "101011000" + "100010100" + // cL 0 9
    "100001010" + "111010110" + "100111010" + "111011010" + // cZ bF
    "110001010" + "111011010" + "110011010" + "110101000" + // bV A Z
    "100111010" + "111011010" + "100011010" + "111011010" + // bK bO
    "100101100" + "111011010" + "101101100" + "100110010" + // bW dA
    "110101000" + "100110010" + "100111010" + "111011010" + // dZ bP
    "100010110" + "111011010" + // bT
    "110100110" + "110100010" + // checksum: 12 28
    "110101100" + "101011110" + "100000");
}
