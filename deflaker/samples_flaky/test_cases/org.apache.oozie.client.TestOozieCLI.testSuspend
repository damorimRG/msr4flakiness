public void testSuspend() throws Exception {
    runTest(END_POINTS, SERVLET_CLASSES, IS_SECURITY_ENABLED, new Callable<Void>() {

        @Override
        public Void call() throws Exception {
            String oozieUrl = getContextURL();
            String[] args = new String[] { "job", "-oozie", oozieUrl, "-suspend", MockDagEngineService.JOB_ID + "1" + MockDagEngineService.JOB_ID_END };
            assertEquals(0, new OozieCLI().run(args));
            assertEquals(RestConstants.JOB_ACTION_SUSPEND, MockDagEngineService.did);
            args = new String[] { "job", "-oozie", oozieUrl, "-suspend", MockDagEngineService.JOB_ID + (MockDagEngineService.workflows.size() + 1) };
            assertEquals(-1, new OozieCLI().run(args));
            return null;
        }
    });
}
